{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Kubernetes: Run/Debug",
      "type": "cloudcode.kubernetes",
      "request": "launch",
      "skaffoldConfig": "${workspaceFolder}/skaffold.yaml",
      "profile": "cloudcode",
      "watch": true,
      "cleanUp": true,
      "portForward": true,
      "imageRegistry": "gcr.io/nnyn-dev",
      "debug": [
        {
          "image": "gcr.io/nnyn-dev/simple-post-server",
          "sourceFileMap": {
            "${workspaceFolder}": "/app"
          }
        }
      ]
    },
    {
      "name": "gisapp intermediate with Kubernetes",
      "type": "python",
      "request": "launch",
      "module": "gisapp",
      "args": ["--host", "0.0.0.0:8080", "--down=backend-service:8080"],
      "preLaunchTask": "bridge-to-kubernetes.resource",
      "env": {
        "GRPC_DNS_RESOLVER": "native"
      }
    },
    {
      "name": "gisapp",
      "type": "python",
      "request": "launch",
      "module": "gisapp",
      "args": ["--host", "0.0.0.0:8080"]
    },
    {
      "name": "gisapp intermediate",
      "type": "python",
      "request": "launch",
      "module": "gisapp",
      "args": ["--host", "0.0.0.0:8080", "--down=backend-service:8080"]
    },
    {
      "name": "client",
      "type": "python",
      "request": "launch",
      "module": "client.client"
    },
    {
      "name": "cloud code attach",
      "type": "cloudcode.kubernetes",
      "request": "attach",
      "language": "Python",
      "debugPort": 3000,
      "podSelector": {
        "app": "intermediate"
      },
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "/app"
    },
    {
      "name": "attach debugpy",
      "type": "python",
      "request": "attach",
      "connect": {
        "host": "localhost",
        "port": 5678
      },
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}",
          "remoteRoot": "."
        }
      ]
    }
  ],
  "compounds": []
}
